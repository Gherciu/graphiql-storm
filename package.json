{
  "name": "graphiql-storm",
  "version": "1.2.1",
  "description": "A graphQl Web IDE",
  "keywords": [
    "graphql",
    "graphiql",
    "graphiql-storm",
    "web-storm",
    "ide",
    "web ide",
    "graph"
  ],
  "repository": {
    "url": "https://github.com/Gherciu/graphiql-storm",
    "type": "git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress",
    "watch": "./node_modules/.bin/webpack --watch",
    "watch:example": "./node_modules/.bin/webpack ./example/index.js  -o ./example/index.bundle.js -w"
  },
  "author": {
    "name": "Gherciu Gheorghe",
    "email": "gherciu553@gmail.com",
    "url": "https://gherciu.github.io/portfolio/"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "graphiql": "^0.10.0",
    "graphql": "^14.0.2",
    "husky": "^3.0.9",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "ignore-loader": "^0.1.2",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
